{
  "name": "haddi",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/index.js",
    "test": "jest",
    "test:mobile": "node scripts/test-mobile-apis.js",
    "test:google-oauth": "node scripts/test-google-oauth.js",
    "test:redis": "node scripts/redis-health-check.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "seed": "ts-node prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:reset": "prisma db push --force-reset",
    "db:studio": "prisma studio",
    "prod:deploy": "./scripts/deploy.sh",
    "prod:backup": "./scripts/backup.sh",
    "prod:monitor": "./scripts/monitor.sh",
    "prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "prod:restart": "docker-compose -f docker-compose.prod.yml restart",
    "prod:update": "git pull && docker-compose -f docker-compose.prod.yml up -d --build",
    "health:check": "node healthcheck.js",
    "redis:health": "node scripts/redis-health-check.js",
    "redis:status": "curl -s http://localhost:4545/api/redis/status | jq",
    "redis:stats": "curl -s http://localhost:4545/api/redis/stats | jq",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d app",
    "docker:dev:build": "docker-compose -f docker-compose.dev.yml up -d --build app",
    "docker:dev:all": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:all:build": "docker-compose -f docker-compose.dev.yml up -d --build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f app",
    "docker:dev:restart": "docker-compose -f docker-compose.dev.yml restart app",
    "monitoring:start": "./scripts/start-monitoring-dev.sh",
    "monitoring:stop": "./scripts/stop-monitoring-dev.sh",
    "monitoring:status": "docker-compose -f docker-compose.monitoring.dev.yml ps",
    "monitoring:logs": "docker-compose -f docker-compose.monitoring.dev.yml logs -f",
    "docker:prod": "docker-compose up app",
    "docker:prod:build": "docker-compose up --build app",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "docker:logs": "docker-compose logs -f app",
    "docker:db:push": "docker-compose -f docker-compose.dev.yml exec app npx prisma db push",
    "docker:db:seed": "docker-compose -f docker-compose.dev.yml exec app npm run seed",
    "docker:db:studio": "docker-compose -f docker-compose.dev.yml exec app npx prisma studio",
    "docker:prod:db:sync": "docker-compose exec app npx prisma db push && docker-compose exec app npx prisma generate && docker-compose exec app npm run seed",
    "docker:prod:db:sync:script": "./scripts/sync-prod-db.sh",
    "create:level1": "ts-node scripts/create-level1-courses.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "@prisma/client": "^6.10.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^10.2.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.3",
    "nodemon": "^3.1.10",
    "prisma": "^6.10.1",
    "prom-client": "^15.1.3",
    "redis": "^4.6.13",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.3",
    "@types/uuid": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
